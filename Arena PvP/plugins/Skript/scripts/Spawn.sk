command /spawncek:
	permission: sk.admin
	trigger:
		set {spawn} to location of player
		send  " &3ArenaPvP &8» &eSpawn Ayarlandi"
	
on join:
	teleport player to {spawn}
on first join:
    teleport player to {spawn}
	
